php_flag display_startup_errors on
php_flag display_errors on
RewriteEngine on
RewriteCond %{REQUEST_URI} !\.(?:css|js)$ [NC]
RewriteRule ^index.php              controllers/dashboard.php
RewriteRule ^login.php              controllers/login.php
RewriteRule ^loginUser.php          controllers/loginUser.php
RewriteRule ^register.php           controllers/register.php
RewriteRule ^registerUser.php       controllers/registerUser.php